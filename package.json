{
  "name": "friendlinks",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "json": "node --loader ts-node/esm ./utils/tojson.ts",
    "remove-favicons": "node ./scripts/remove-favicons.mjs",
    "check-access": "bun ./scripts/check-access.ts",
    "watch": "node ./utils/watch.ts",
    "dev": "npm run json && concurrently \"npm run watch\" \"astro dev\"",
    "bundle-client": "node ./scripts/bundle-client.mjs",
    "build": "npm run json && npm run bundle-client && astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@edgeone/astro": "^1.0.4",
    "astro": "^5.16.4",
    "chokidar": "^5.0.0",
    "concurrently": "^9.2.1",
    "decimal.js": "^10.6.0",
    "fuse.js": "^7.1.0",
    "graphology": "^0.26.0",
    "graphology-layout-forceatlas2": "^0.10.1",
    "graphology-utils": "^2.5.2",
    "sigma": "^3.0.2",
    "yaml": "^2.8.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
